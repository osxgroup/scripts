<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>OnDemand</key>
	<false/>
	<key>WorkingDirectory</key>
	<string>/usr/local/topicdesk/awl</string>
	<key>Label</key>
	<string>com.topicdesk.awl</string>
	<key>ProgramArguments</key>
	<array>
		<string>./awl.pl</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/tmp/com.topicdesk.awl.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.topicdesk.awl.stdout</string>
</dict>
</plist>
